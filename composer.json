{
  "name": "delegator/magerock",
  "type": "project",
  "license": "Apache-2.0",
  "description": "A modern Magento CE 1.x stack",
  "homepage": "https://www.delegator.com",
  "config": {
    "preferred-install": "dist"
  },
  "support": {
    "issues": "https://github.com/delegator/magerock/issues"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.firegento.com"
    },
    {
      "type": "path",
      "url": "third-party/*/*"
    },
    {
      "type": "path",
      "url": "packages/*/*"
    }
  ],
  "require": {
    "php": ">=5.4",
    "aydin-hassan/magento-core-composer-installer": "1.3.0",
    "firegento/magento": "1.9.2.4",
    "magento-hackathon/magento-composer-installer": "3.0.6"
  },
  "require-dev": {
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-gd": "*",
    "ext-hash": "*",
    "ext-iconv": "*",
    "ext-mcrypt": "*",
    "ext-pcre": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "ext-simplexml": "*"
  },
  "extra": {
    "magento-root-dir": "web",
    "magento-core-deploy" : {
      "excludes": [
        "dev",
        "downloader",
        "media",
        "var/log",
        "var/cache",
        ".htaccess"
      ]
    }
  },
  "autoload": {
    "psr-0": {
      "": [
        "web/app",
        "web/app/code/local",
        "web/app/code/community",
        "web/app/code/core",
        "web/lib"
      ]
    }
  },
  "scripts": {
    "pre-install-cmd": [
      "mkdir -p web"
    ]
  }
}
